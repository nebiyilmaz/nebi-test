  "authors": [
    {
      "name": "Anders Hallsten",
      "email": "anders.hallsten@divido.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/api-error-lib-php.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/log-streamer-lib-php.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/rpc-client-lib-php.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/merchant-api-sdk.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/application-api-sdk.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/waterfall-api-sdk.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/lender-communication-api-sdk.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dividohq/cache-lib-php.git"
