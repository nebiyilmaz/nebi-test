      "url": "git@github.com:dividohq/index-rate-api-sdk-php.git"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "slim/slim": "^3",
    "monolog/monolog": "^2.0",
    "aws/aws-sdk-php": "^3",
    "symfony/console": "^5",
    "guzzlehttp/guzzle": "^6",
    "divido/api-error-lib": "^1.1.1",
    "justinrainbow/json-schema": "^5",
    "divido/log-streamer": "^3.0",
    "divido/merchant-api-sdk": "^2",
    "divido/application-api-sdk": "^3",
    "divido/lender-communication-api-sdk": "^4.1.1",
    "divido/rpc-client": "1.0.0",
    "psr/http-message": "1.0.1",
    "hassankhan/config": "^2",
    "ramsey/uuid": "^3.7",
    "divido/cache": "^1.1.0",
    "php-http/guzzle6-adapter": "^2.0",
    "divido/index-rate-api-sdk": "^1.0",
    "divido/waterfall-api-sdk": "^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "hamcrest/hamcrest-php": "^2.0",
    "mockery/mockery": "^1.3",
    "php-http/mock-client": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Divido\\": "src/Divido"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Divido\\Test\\Unit\\": "tests/unit",
      "Divido\\Test\\Functional\\": "tests/functional/tests"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
